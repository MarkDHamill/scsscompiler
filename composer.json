{
    "name": "phpbbservices/scsscompiler",
    "type": "phpbb-extension",
    "description": "Compiles phpBB SCSS-based styles",
    "homepage": "https://www.phpbbservices.com/",
    "version": "1.0.0-dev",
    "time": "2021-11-25",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "MarkDHamill",
            "email": "mark@phpbbservices.com",
            "homepage": "https://www.phpbbservices.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "composer/installers": "~1.0",
        "scssphp/scssphp": "^1.8.1"
    },
    "extra": {
        "display-name": "SCSS Compiler",
        "soft-require": {
            "phpbb/phpbb": ">=3.3.0,<3.4.0@dev"
        },
        "version-check": {
            "host": "www.phpbbservices.com",
            "directory": "/scsscompiler/updatecheck/",
            "filename": "3.3.json",
            "ssl": true
        }
    }
}
